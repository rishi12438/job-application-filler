{
  "name": "Job app filler ",
  "version": "0.1",
  "description": "A job app filler",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "details.html"
  },
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*", "file://*/*" ],
      "all_frames": true,
      "js": ["app.js","jquery.js","jquery-ui.js"],
      "css":["content.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "manifest_version": 2,
  "web_accessible_resources": ["img/0.jpeg"],
  "content_security_policy": "script-src 'self' 'sha256-p5Qu8LTVrgxFnVPkA9HOlRGZ9NNhM6/fyu5PNmPm0dw='"
}
